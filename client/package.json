{
  "name": "nexus-client",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "jest --detectOpenHandles --verbose",
    "test:update": "jest --updateSnapshot",
    "test:a11y": "jest --config jest.a11y.config.ts",
    "test:migration": "jest __tests__/integration/migration-regression.test.ts",
    "test:ai-service": "jest __tests__/core/AIService.test.ts",
    "test:smoke": "jest __tests__/integration/migration-regression.test.ts --verbose",
    "rls-test": "jest __tests__/security/rls.test.ts",
    "type-check": "tsc --noEmit",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run",
    "analyze:consistency": "node scripts/analyze-consistency.cjs",
    "fix:consistency": "node scripts/fix-consistency.cjs",
    "test:consistency": "npm run analyze:consistency && node -e \"const fs=require('fs');const report=fs.readFileSync('docs/CONSISTENCY_REPORT.md','utf8');const score=parseFloat(report.match(/Consistency Score:\\*\\* (\\d+\\.?\\d*)/)?.[1]||0);if(score<7.0){console.error('❌ Consistency score',score,'below threshold 7.0');process.exit(1)}else{console.log('✅ Consistency score',score,'meets requirements')}\"",
    "worker": "node scripts/start-worker.js",
    "scan:models": "node scripts/scan-models.cjs",
    "fix:colors": "node scripts/fix-hardcoded-colors.cjs",
    "fix:colors:dry": "node scripts/fix-hardcoded-colors.cjs --dry-run",
    "cleanup:deps": "npm uninstall @jridgewell/gen-mapping @kernel.salacoste/n8n-workflow-builder @radix-ui/react-progress @radix-ui/react-slot pino-pretty shadcn shadcn-ui && npm uninstall --save-dev @testing-library/user-event @types/jest autoprefixer jest-environment-jsdom tw-animate-css",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate-sitemap": "node scripts/generate-sitemap.cjs",
    "generate-robots": "node scripts/generate-robots.cjs",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "lint:summary": "eslint . --ext ts,tsx --format=compact | grep -E '(error|warning)' | wc -l",
    "lint:errors-only": "eslint . --ext ts,tsx --format=compact | grep 'error' | wc -l",
    "verify:db-patterns": "node scripts/verify-database-patterns.cjs",
    "migrate:db-patterns": "node scripts/migrate-supabase-to-postgres.cjs",
    "check:imports": "node scripts/proactive-import-checker-fixed.js",
    "check:imports:fix": "node scripts/proactive-import-checker-fixed.js --fix",
    "check:imports:strict": "node scripts/proactive-import-checker-fixed.js --strict",
    "check:imports:quick": "node scripts/quick-import-check.js",
    "fix:postgres-imports": "./scripts/fix-postgres-imports.sh",
    "ci:import-check": "./scripts/ci-import-check.sh",
    "cleanup:scripts": "./scripts/cleanup-redundant-scripts.sh",
    "cleanup:scripts:dry": "./scripts/cleanup-redundant-scripts.sh --dry-run",
    "kill": "pkill -f 'vite\\|node.*dev\\|pnpm.*dev' || echo 'No development processes found'",
    "kill:all": "pkill -f node || echo 'No Node.js processes found'",
    "kill:ports": "lsof -ti:3000,5173,5432 | xargs kill -9 2>/dev/null || echo 'No processes found on ports 3000, 5173, 5432'",
    "db:reset": "psql \"$DATABASE_URL\" -f reset_database.sql",
    "db:setup": "psql \"$DATABASE_URL\" -f schema_cleanup_migration.sql",
    "dev:setup": "pnpm db:setup",
    "dev:reset": "pnpm db:reset && pnpm db:setup",
    "db:sync:from-remote": "./scripts/sync-database.sh sync-from-remote",
    "db:sync:to-remote": "./scripts/sync-database.sh sync-to-remote",
    "db:sync:create-migration": "./scripts/sync-database.sh create-migration",
    "db:sync:status": "./scripts/sync-database.sh check-status",
    "db:sync:help": "./scripts/sync-database.sh help",
    "db:repair-migrations": "./scripts/repair-migrations.sh",
    "db:quick-repair": "./scripts/quick-repair.sh",
    "db:wsl-repair": "./scripts/wsl-repair.sh",
    "sync:to-remote": "./scripts/sync-to-remote.sh",
    "sync:from-remote": "./scripts/pull-from-remote.sh",
    "cleanup:check": "node scripts/cleanup-dependencies.js",
    "cleanup:remove": "pnpm remove @azure/msal-browser @microsoft/mgt-components @microsoft/mgt-element @microsoft/mgt-msal2-provider @microsoft/mgt-react react-hot-toast",
    "setup:hubspot": "node scripts/setup-hubspot-env.js",
    "seed:demo": "node scripts/seed-demo-data.js"
  },
