{
  "name": "nexus-dashboard",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "jest --detectOpenHandles --verbose",
    "test:update": "jest --updateSnapshot",
    "rls-test": "jest __tests__/security/rls.test.ts",
    "type-check": "tsc --noEmit",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run",
    "analyze:consistency": "node scripts/analyze-consistency.cjs",
    "fix:consistency": "node scripts/fix-consistency.cjs",
    "test:consistency": "npm run analyze:consistency && node -e \"const fs=require('fs');const report=fs.readFileSync('docs/CONSISTENCY_REPORT.md','utf8');const score=parseFloat(report.match(/Consistency Score:\\*\\* (\\d+\\.?\\d*)/)?.[1]||0);if(score<7.0){console.error('❌ Consistency score',score,'below threshold 7.0');process.exit(1)}else{console.log('✅ Consistency score',score,'meets requirements')}\"",
    "demo:ai": "tsx scripts/ai-transformation-demo.ts",
    "demo:ai:enhanced": "tsx scripts/ai-transformation-demo.ts --enhanced",
    "scan:models": "node scripts/scan-models.cjs",
    "fix:colors": "node scripts/fix-hardcoded-colors.cjs",
    "fix:colors:dry": "node scripts/fix-hardcoded-colors.cjs --dry-run",
    "cleanup:deps": "npm uninstall @jridgewell/gen-mapping @kernel.salacoste/n8n-workflow-builder @radix-ui/react-progress @radix-ui/react-slot pino-pretty shadcn shadcn-ui && npm uninstall --save-dev @testing-library/user-event @types/jest autoprefixer jest-environment-jsdom supabase tw-animate-css",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "pnpm": {
    "allowed-build-scripts": [
      "supabase"
    ],
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "cypress",
      "esbuild",
      "prisma"
    ]
  },
