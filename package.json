{
  "name": "nexus",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "test:debug": "jest --detectOpenHandles --verbose",
    "test:update": "jest --updateSnapshot",
    "type-check": "tsc --noEmit",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run",
    "analyze:consistency": "node scripts/analyze-consistency.cjs",
    "fix:consistency": "node scripts/fix-consistency.cjs",
    "test:consistency": "npm run analyze:consistency && node -e \"const fs=require('fs');const report=fs.readFileSync('docs/CONSISTENCY_REPORT.md','utf8');const score=parseFloat(report.match(/Consistency Score:\\*\\* (\\d+\\.?\\d*)/)?.[1]||0);if(score<7.0){console.error('❌ Consistency score',score,'below threshold 7.0');process.exit(1)}else{console.log('✅ Consistency score',score,'meets requirements')}\"",
    "demo:ai": "tsx scripts/ai-transformation-demo.ts",
    "demo:ai:enhanced": "tsx scripts/ai-transformation-demo.ts --enhanced"
  },
  "dependencies": {
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@supabase/supabase-js": "^2.49.10",
    "@types/axios": "^0.9.36",
    "@types/recharts": "^1.8.29",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.16.0",
    "lucide-react": "^0.511.0",
    "prop-types": "^15.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.0",
    "recharts": "^2.15.3",
    "shadcn": "^2.5.0",
    "shadcn-ui": "^0.9.5",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.21",
    "@types/prop-types": "^15.7.14",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "glob": "^11.0.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.3.4",
    "tsx": "^4.7.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
